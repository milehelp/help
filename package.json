{
  "name": "tiecode",
  "version": "1.0.0",
  "private": true,
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "scripts": {
    "start": "yarn run dev",
    "dev": "dumi dev",
    "build": "dumi build",
    "prepare": "husky install && dumi setup"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "yarn": "^1.22.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "dumi": "^2.2.16",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
